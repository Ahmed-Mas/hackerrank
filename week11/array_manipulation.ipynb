     "output_type": "execute_result"
    }
   ],
   "source": [
    "arr = [0] * (n + 2)\n",
    "\n",
    "for a, b, k in queries:\n",
    "    arr[a] += k\n",
    "    arr[b + 1] -= k\n",
    "\n",
    "for i in range(1, len(arr)):\n",
    "    arr[i] += arr[i - 1]\n",
    "arr"
